@model IEnumerable<User>

<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <!-- Other user properties -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <!-- Other user properties -->
            </tr>
        }
    </tbody>
</table>
